void upore_dekho()
{
   string str;
   cin>>str;
   for(int i=0;i<str.size();i++)
   {
       if(str[i]=='9' && i==0) continue;
       else if(str[i]>'4') str[i]='9'+'0'-str[i];
   }
   cout<<str<<endl;
 
}
int main()
{
    IOS;
 
    int t=1;
   // cin>>t;
 
    for(int cs=1;cs<=t;cs++)
    {
        /// cout<<"Case "<<cs<<": Yes"<<endl;
        upore_dekho();
    }
 
    return 0;
}
